{
	"name" : "r3m_io/framework",
	"description" : "R3M framework see https://r3m.io for detailed usage",
	"type" : "library",
	"keywords" : ["framework", "r3m", "r3m.io", "library", "language", "template", "engine"],
	"homepage": "https://r3m.io",
	"require" : {
		"php": ">=8.0",
		"ext-mbstring": "*",
		"ext-json": "*",
		"ext-pdo": "*",
		"ext-posix": "*",
		"ext-zend-opcache": "*",
		"ext-openssl": "*",
		"ext-readline": "*",
		"ext-pcntl": "*",
		"ext-zlib": "*",
		"ext-zip": "*",
		"ext-shmop": "*",
		"r3m_io/autoload": ">=2024.05.25",
		"r3m_io/boot": ">=2024.05.25",
		"r3m_io/basic": ">=2024.05.25",
		"r3m_io/event": ">=2024.05.25",
		"r3m_io/config": ">=2024.05.25",
		"r3m_io/host": ">=2024.05.25",
		"r3m_io/middleware": ">=2024.05.25",
		"r3m_io/node": ">=2024.05.25",
		"r3m_io/output_filter": ">=2024.05.25",
		"r3m_io/log": ">=2024.05.25",
		"r3m_io/route": ">=2024.05.25",
		"r3m_io/server": ">=2024.05.25",
		"r3m_io/account": ">=2024.05.25",
		"monolog/monolog" : "^3.6",
		"defuse/php-encryption": "^2.3",
		"spatie/fork": "^1.2.2"
    },
	"require-dev" : {
		"pestphp/pest": "^2.34"
	},
	"config": {
		"allow-plugins": {
			"pestphp/pest-plugin": true
		}
	},
	"license" : "MIT",
	"authors" : [{
			"name" : "Remco van der Velde",
			"email" : "development@universeorange.com"
		}
	],
	"autoload" : {
		"psr-4" : {
			"R3m\\Io\\" : "src/"
		}
	}
}