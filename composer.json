	"homepage": "https://r3m.io",
	"require" : {
		"php": ">=8.0",
		"ext-mbstring": "*",
		"ext-json": "*",
		"ext-pdo": "*",
		"ext-posix": "*",
		"ext-zend-opcache": "*",
		"ext-openssl": "*",
		"ext-readline": "*",
		"ext-pcntl": "*",
		"ext-zlib": "*",
		"ext-zip": "*",
		"ext-shmop": "*",
		"r3m_io/autoload": ">=2024.05.25",
		"r3m_io/boot": ">=2024.05.25",
		"r3m_io/basic": ">=2024.05.25",
		"r3m_io/event": ">=2024.05.25",
		"r3m_io/config": ">=2024.05.25",
		"r3m_io/host": ">=2024.05.25",
		"r3m_io/middleware": ">=2024.05.25",
		"r3m_io/node": ">=2024.05.25",
		"r3m_io/output_filter": ">=2024.05.25",
